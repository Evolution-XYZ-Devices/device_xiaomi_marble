<resources>
    <!-- Device specific doze package -->
    <string name="config_customDozePackage" translatable="false">org.lineageos.settings/org.lineageos.settings.doze.DozeSettingsActivity</string>

    <!-- Whether to show the Preference for Adaptive connectivity -->
    <bool name="config_show_adaptive_connectivity">false</bool>

    <!-- Whether to show Minimum and Maximum refresh rate in display settings -->
    <bool name="config_show_refresh_rate_controls">true</bool>
    <bool name="config_supports_dynamic_refresh_rate_controls">true</bool>

    <!-- Battery health -->
    <bool name="config_supportBatteryHealth">true</bool>
    <string name="config_batChargeCycle">/sys/class/power_supply/battery/cycle_count</string>
    <string name="config_batCurCap">/sys/class/power_supply/battery/charge_full</string>
    <string name="config_batDesCap">/sys/class/power_supply/battery/charge_full_design</string>
</resources>
